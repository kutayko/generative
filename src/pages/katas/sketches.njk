---
pagination:
    data: sketches
    size: 1
    alias: sketch
permalink: "katas/{{ sketch.title | slug }}/"
tag: sketch
---
<!doctype html>
<html lang="en">

  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ sketch.title }}</title>
    <link rel="stylesheet" href="{{ assetsFolder }}/stylesheets/styles.css">

    <script src="{{ assetsFolder }}/p5.min.js"></script>
    <script src="{{ sketchesFolder }}/{{ sketch.title | slug }}.js"></script>
  </head>

  <body>
    <header>
        <div id="navbar">
            <a href="..">katas</a>
        </div>

        <h1>{{ sketch.title }}</h1>
        <p>{{ sketch.created_at.toDateString() }}</p>
    </header>
    <main>
        <div id="sketch-container">
        </div>
    </main>
  </body>

</html>
